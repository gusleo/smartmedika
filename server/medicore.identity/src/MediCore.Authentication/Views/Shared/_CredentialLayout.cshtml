<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SmartMedika</title>
    <environment include="Development">
        <link rel="stylesheet" href="~/vendor/bootstrap/css/bootstrap.min.css" />
        <link rel="stylesheet" type="text/css" href="~/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
        <link rel="stylesheet" type="text/css" href="~/fonts/iconic/css/material-design-iconic-font.min.css">
        <link rel="stylesheet" type="text/css" href="~/vendor/animate/animate.css">
        <link rel="stylesheet" type="text/css" href="~/vendor/css-hamburgers/hamburgers.min.css">
        <link rel="stylesheet" type="text/css" href="~/vendor/animsition/css/animsition.min.css">
        <link rel="stylesheet" type="text/css" href="~/vendor/select2/select2.min.css">
        <link rel="stylesheet" type="text/css" href="~/vendor/daterangepicker/daterangepicker.css">
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/4.0.0/css/bootstrap.min.css"
              asp-fallback-href="~/vendor/bootstrap/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
              asp-fallback-href="~/fonts/font-awesome-4.7.0/css/font-awesome.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/material-design-iconic-font/2.2.0/css/material-design-iconic-font.min.css"
              asp-fallback-href="~/fonts/iconic/css/material-design-iconic-font.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css"
              asp-fallback-href="~/vendor/animate/animate.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/hamburgers/0.9.3/hamburgers.min.css"
              asp-fallback-href="~/vendor/css-hamburgers/hamburgers.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animsition/4.0.2/css/animsition.min.css"
              asp-fallback-href="~/vendor/animsition/css/animsition.min.css" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.5/css/select2.min.css"
              asp-fallback-href="~/vendor/select2/select2.min.css" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-daterangepicker/2.1.25/daterangepicker.min.css"
              asp-fallback-href="~/vendor/daterangepicker/daterangepicker.css" />
    </environment>
    <link rel="stylesheet" type="text/css" href="~/css/credential/util.css" />
    <link rel="stylesheet" type="text/css" href="~/css/credential/main.css" />
</head>
<body>
	<div class="limiter">
		<div class="container-login100" style="background-image: url('../images/bg-01.jpg');">
			@RenderBody()
		</div>
	</div>
	<environment include="Development">
		<script src="~/vendor/jquery/jquery-3.2.1.min.js" asp-append-version="true"></script>
		<script src="~/vendor/animsition/js/animsition.min.js" asp-append-version="true"></script>
		<script src="~/vendor/bootstrap/js/popper.js" asp-append-version="true"></script>
		<script src="~/vendor/bootstrap/js/bootstrap.min.js" asp-append-version="true"></script>
		<script src="~/vendor/select2/select2.min.js" asp-append-version="true"></script>
		<script src="~/vendor/daterangepicker/moment.min.js" asp-append-version="true"></script>
		<script src="~/vendor/daterangepicker/daterangepicker.js" asp-append-version="true"></script>
	</environment>
	<environment exclude="Development">
		<script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.2.1.min.js"
				asp-fallback-src="~/vendor/jquery/jquery-3.2.1.min.js"
				asp-fallback-test="window.jQuery"
				crossorigin="anonymous"
				integrity="sha384-K+ctZQ+LL8q6tP7I94W+qzQsfRV2a+AfHIi9k8z8l9ggpc8X+Ytst4yBo/hH+8Fk">
		</script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/animsition/4.0.2/js/animsition.min.js"
				asp-fallback-src="~/vendor/animsition/js/animsition.min.js">
		</script>
		<script src="https://ajax.aspnetcdn.com/ajax/bootstrap/4.0.0/popper.min.js"
				asp-fallback-src="~/vendor/bootstrap/js/popper.js"
				asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
				crossorigin="anonymous"
				integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
		</script>
		<script src="https://ajax.aspnetcdn.com/ajax/bootstrap/4.0.0/bootstrap.min.js"
				asp-fallback-src="~/vendor/bootstrap/js/bootstrap.min.js"
				asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
				crossorigin="anonymous"
				integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
		</script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.5/js/select2.min.js"
				asp-fallback-src="~/vendor/select2/select2.min.js">
		</script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-daterangepicker/2.1.25/moment.min.js"
				asp-fallback-src="~/vendor/daterangepicker/moment.min.js">
		</script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-daterangepicker/2.1.25/daterangepicker.min.js"
				asp-fallback-src="~/vendor/daterangepicker/daterangepicker.js">
		</script>
	</environment>

    <script src="~/vendor/countdowntime/countdowntime.js" asp-append-version="true"></script>
    <script src="~/js/credential/main.js" asp-append-version="true"></script>
	@RenderSection("Scripts", required: false)
</body>
</html>
